{
  "name": "s-material-react",
  "version": "0.1.13",
  "description": "'组件库'",
  "main": "dist/index.js",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "dist",
    "css",
    "iconfont"
  ],
  "scripts": {
    "start": "concurrently npm:start:*",
    "start:ts": "npx tsc --watch",
    "start:css": "npx nodemon --config ./nodemon.json",
    "cp-css": "rm -rf css/ && mkdir -p ./css && cp -R src/scss/* ./css",
    "build": "npm run cp-css && rm -rf dist/ && prettier --write src/ && tsc",
    "push": "git add . && git commit -m",
    "postpush": "git push",
    "prepublishOnly": "echo 我们准备发布npm包",
    "publishOnly": "npm run build && npm run push",
    "postpublishOnly": "npm version patch && npm publish && git push"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@brushes/api": "^0.3.3",
    "@brushes/components": "^0.0.40",
    "@brushes/qj-simulate-component": "^0.0.9",
    "@brushes/store": "^1.0.46",
    "@brushes/tools": "^1.0.31",
    "@stencil/core": "^2.19.1",
    "@types/node": ">18.0.0",
    "@types/react": "^18.0.0",
    "china-division": "^2.5.0",
    "classnames": "^2.3.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "react": "^18.0.0",
    "typescript": ">4.0.0"
  },
  "devDependencies": {
    "concurrently": "^7.5.0"
  }
}
